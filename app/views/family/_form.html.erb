<%= form_for(@family, url: url) do |f| %>
  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>
  <div class="form-group">
    <%= f.label :email %>
    <%= f.email_field :email %>
  </div>
  <div class="form-group">
    <%= f.label :phone %>
    <%= f.phone_field :phone %>
  </div>
  <div class="form-group">
    <%= f.label :city %>
    <%= f.text_field :city %>
  </div>
  <div class="form-group">
    <%= f.label :address %>
    <%= f.text_field :address %>
  </div>
  <%= f.hidden_field :contacted,value: true %>
  <%= f.submit "Submit", class: "waves-effect waves-light btn" %>
<% end %>
